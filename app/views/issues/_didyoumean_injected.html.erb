<% content_for :header_tags do %>
  <%= stylesheet_link_tag 'didyoumean', :plugin => 'redmine_didyoumean' %>
  <%= javascript_include_tag 'didyoumean', :plugin => 'redmine_didyoumean' %>
<% end %>

<script type="text/javascript">
  var dym = {
    label_more: "<%= l(:dym_more_issues) %>",
    label_in: "<%= l(:dym_in_project) %>",
    issue_url: "<%= url_for(Issue.new) %>",
    search_url: "<%= url_for(:action => 'index', :controller => 'search_issues', :format => 'json') %>"
  };

  $('issue_subject').up().insert({after: '<%= escape_javascript(render('similar_issues')) %>'});

  observeIssueSubjectField("<%= @project.id %>",
    "<%= issues_didyoumean_event_type %>");
</script>
